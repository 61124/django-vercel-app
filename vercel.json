{
  "version": 2,
  "builds": [
    {
      "src": "support_system/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "support_system/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "support_system.settings",
      "PYTHONUNBUFFERED": "1"
    },
    "cmd": "pip install -r requirements.txt"
  }
}
